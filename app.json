{
  "expo": {
    "name": "CTLMonster",
    "slug": "tss-monseter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "ctlmonster",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.akiq82.ctlmonster",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "name": "DigiRide",
      "shortName": "DigiRide",
      "themeColor": "#1a1a2e",
      "backgroundColor": "#1a1a2e"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "react-native-health-connect",
        {
          "requestPermissions": [
            "android.permission.health.READ_STEPS",
            "android.permission.health.READ_FLOORS_CLIMBED"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "91bdaabb-9520-458d-b520-4873dba740e6"
      }
    },
    "owner": "redpark82s-organization"
  }
}
